{
  "schemaVersion": 1,
  "id": "noseedmining",
  "version": "${version}",
  "name": "NoSeedMining",
  "description": "Prevents seed-based ore prediction tools by mixing a server-secret into ore generation RNG",
  "authors": ["bacon"],
  "contact": {
    "sources": "https://github.com/yourusername/NoSeedMining"
  },
  "license": "Apache-2.0",
  "icon": "assets/noseedmining/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "org.bacon.noseedmining.client.NoseedminingDataGenerator"
    ],
    "main": [
      "org.bacon.noseedmining.Noseedmining"
    ]
  },
  "mixins": [
    "noseedmining.mixins.json",
    {
      "config": "noseedmining.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
